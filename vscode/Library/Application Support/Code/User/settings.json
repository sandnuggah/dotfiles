{
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "none",
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "none",
  "editor.overviewRulerBorder": false,
  "editor.folding": false,
  "editor.occurrencesHighlight": false,
  "editor.matchBrackets": false,
  "editor.glyphMargin": true,
  "explorer.openEditors.visible": 0,
  "workbench.activityBar.visible": true,
  "workbench.editor.showIcons": false,
  "workbench.editor.tabCloseButton": "off",
  "indenticator.color.dark": "rgba(255,255,255,.1)",
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#0000"
  },
  "breadcrumbs.enabled": false,
  "editor.renderControlCharacters": false,
  "prettier.jsxBracketSameLine": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "window.zoomLevel": 0,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "editor.formatOnSave": true,
  "python.formatting.provider": "black",
  "C_Cpp.updateChannel": "Insiders",
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.sideBar.location": "right",
  "diffEditor.renderSideBySide": true,
  "workbench.statusBar.visible": true,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "rust-client.rustupPath": "/Users/adam/.cargo/bin/rustup",
  "rust-client.updateOnStartup": true,
  "git.confirmSync": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "debug.toolBarLocation": "docked",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "telemetry.enableTelemetry": false,
  "platformio-ide.activateOnlyOnPlatformIOProject": true,
  "editor.hideCursorInOverviewRuler": true,
  "bracket-pair-colorizer-2.colors": [
    "Orchid",
    "HotPink",
    "Purple",
    "Salmon",
  ],
  "editor.scrollBeyondLastLine": false,
  "C_Cpp.default.cppStandard": "c++11",
  "flow.enabled": false,
  "autoDarkMode.darkTheme": "Spacemacs - dark",
  "autoDarkMode.lightTheme": "Spacemacs - light",
  "workbench.colorTheme": "Horizon Bold",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[toml]": {},
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "auto-close-tag.enableAutoCloseSelfClosingTag": false,
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "python.jediEnabled": false,
  "python.analysis.disabled": [
    "unresolved-import",
    "undefined-variable",
  ],
}